---
import { getEntry } from 'astro:content';
import Page from '../components/Page.astro';
import Prose from '../components/Prose.astro';

const welcomeBlurb = await getEntry('homepage', 'homepage');
const { Content } = await welcomeBlurb.render();
---

<Page tabTitle="Home" pageTitle="Welcome">
    <Prose title={welcomeBlurb.data.title}>
        <Content />
    </Prose>
</Page>