---
import Header from '../components/Header.astro';
import Body from '../components/Body.astro';

const { pageTitle } = Astro.props;
---

<div id="wrapper" class="max-w-screen-lg m-auto shadow-[0_0_10px_10px_theme(colors.slate.500/50%)] outline outline-2 outline-slate-500">
    <Header/>
    <Body pageTitle={pageTitle}>
        <slot />
    </Body>
</div>